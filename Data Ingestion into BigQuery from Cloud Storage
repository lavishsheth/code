DATASET_NAME=work_day

TABLE_NAME=employee

BUCKET_NAME=qwiklabs-gcp-04-5b35b5e638a7-pn9v-bucket


bq mk $DATASET_NAME


bq mk --table $DATASET_NAME.$TABLE_NAME employee_id:INTEGER,device_id:STRING,username:STRING,department:STRING,office:STRING


bq load --source_format=CSV --skip_leading_rows=1 $DATASET_NAME.$TABLE_NAME gs://$BUCKET_NAME/employees.csv employee_id:INTEGER,device_id:STRING,username:STRING,username:STRING,department:STRING,office:STRING


bq load --source_format=CSV --skip_leading_rows=1 $DATASET_NAME.$TABLE_NAME gs://$BUCKET_NAME/employees.csv employee_id:INTEGER,device_id:STRING,username:STRING,department:STRING,office:STRING

bq query --use_legacy_sql=false "SELECT * FROM $DATASET_NAME.$TABLE_NAME LIMIT 10"
